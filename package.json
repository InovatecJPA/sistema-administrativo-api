{
	"name": "auth-api",
	"version": "1.0.0",
	"main": "src/server.ts",
	"repository": "https://github.com/Inovatecjp/Authentication-API.git",
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.645.0",
		"@aws-sdk/s3-request-presigner": "^3.645.0",
		"axios": "^1.7.7",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.3",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.21.0",
		"express-handlebars": "^8.0.1",
		"fast-glob": "^3.3.2",
		"helmet": "^7.1.0",
		"http-status-codes": "^2.3.0",
		"jsonwebtoken": "^9.0.2",
		"moment": "^2.30.1",
		"nodemailer": "^6.9.14",
		"nodemailer-express-handlebars": "^6.1.2",
		"pg": "^8.4.0",
		"reflect-metadata": "^0.2.2",
		"swagger-ui-express": "^5.0.1",
		"tsoa": "^6.4.0",
		"typeorm": "^0.3.20",
		"typeorm-naming-strategies": "^4.1.0",
		"typescript": "^5.5.4",
		"zod": "^3.23.8"
	},
	"scripts": {
		"start": "nodemon src/server.js",
		"dev": "cross-env NODE_ENV=dev env-cmd -f .env.development nodemon src/server.js",
		"build": "tsc -p .",
		"swagger": "tsoa spec",
		"typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
		"typeorm:gen-migrations": "node -e \"try { require('child_process').execSync('ts-node-dev ./node_modules/typeorm/cli.js migration:generate -d src/database/dbConnection.ts -o src/database/migrations/' + process.argv[1], { stdio: 'inherit' }) } catch (err) { console.error('No changes detected or other error occurred:', err.message); }\"",
		"typeorm:run-migrations": "ts-node-dev ./node_modules/typeorm/cli.js migration:run -d src/database/dbConnection.ts",
		"typeorm:drop": "ts-node-dev ./node_modules/typeorm/cli.js schema:drop -d src/database/dbConnection.ts"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts",
		"exec": "ts-node src/server.ts"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.6",
		"@types/body-parser": "^1.19.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.13",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/node": "^22.0.2",
		"@types/nodemailer": "^6.4.15",
		"@types/nodemailer-express-handlebars": "^4.0.5",
		"@types/swagger-ui-express": "^4.1.6",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.4.2",
		"env-cmd": "^10.1.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.4",
		"ts-jest": "^29.2.5",
		"ts-node": "10.9.1",
		"ts-node-dev": "^2.0.0"
	}
}
